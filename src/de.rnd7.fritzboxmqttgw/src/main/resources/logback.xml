<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="de.bausdorf.avm" level="warn" />

    <root level="info">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
